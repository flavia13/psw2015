<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>servlet.HelloWorld</servlet-class>
        <init-param>
            <param-name>paramContatore</param-name>
            <param-value>100</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ListaProfessori</servlet-name>
        <servlet-class>servlet.ListaProfessori</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaCorsi</servlet-name>
        <servlet-class>servlet.ListaCorsi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaProfGestita</servlet-name>
        <servlet-class>servlet.ListaProfGestita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaProfJPA</servlet-name>
        <servlet-class>servlet.ListaProfJPA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaProfEJB</servlet-name>
        <servlet-class>servlet.ListaProfEJB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaProfessori</servlet-name>
        <url-pattern>/ListaProfessori</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaCorsi</servlet-name>
        <url-pattern>/ListaCorsi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaProfGestita</servlet-name>
        <url-pattern>/ListaProfGestita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaProfJPA</servlet-name>
        <url-pattern>/ListaProfJPA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaProfEJB</servlet-name>
        <url-pattern>/ListaProfEJB</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
